{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/@prisma/client/**"
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}